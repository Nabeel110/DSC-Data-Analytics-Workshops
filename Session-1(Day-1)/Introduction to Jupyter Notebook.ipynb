    "import random\n",
    "magic_num = random.randint(1,101)\n",
    "print('\\t\\t\\t\\t\\t\\t\\t Guessing Game\\t\\t')\n",
    "print('\\t\\t\\t\\t\\t\\t\\tBy Nabeel Ahmed')\n",
